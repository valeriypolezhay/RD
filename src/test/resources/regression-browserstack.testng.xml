<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Regression BrowserStack" >
<!--    parallel="tests" thread-count="2"-->
    <test name="NoLoginTest" >
        <parameter name="driverName" value="BSdriver" />
        <parameter name="deviceName" value="Google Pixel" />
        <parameter name="screenOrientation" value="portrait" />
        <classes>
            <class name="tests.regression.AuthorizeTest" />
        </classes>
    </test>

    <!--    5641a5a7-->
    <test name="LoginTest" >
        <parameter name="driverName" value="BSdriver" />
        <parameter name="deviceName" value="Google Pixel" />
        <parameter name="screenOrientation" value="portrait" />
        <classes>
            <class name="tests.regression.LoginTest" />
        </classes>
    </test>

    <test name="UnsubscribeTest" >
        <parameter name="driverName" value="BSdriver" />
        <parameter name="deviceName" value="Google Pixel" />
        <parameter name="screenOrientation" value="portrait" />
        <classes>
            <class name="tests.regression.UnsubscribeTest" />
        </classes>
    </test>




</suite>